  <h1 class="font-bold text-4xl">Your Drills</h1>
  <table>
    <caption class="text-left">These are your drills.</caption>
    <thead>
      <tr>
        <th>Date</th>
        <th>Correct</th>
        <th>Incorrect</th>
        <th>Pass</th>
        <th>Total</th>
        <th>% Correct</th>
      </tr>
    </thead>
    <tbody>
      <% @drills.each do |drill| %>
        <tr>
          <td><%= drill.started_at.to_date %></td>
          <td><%= drill.correct_count %></td>
          <td><%= drill.incorrect_count %></td>
          <td><%= drill.pass_count %></td>
          <td><%= drill.clues_seen_count %></td>
          <td><%= drill.correct_count.to_f / drill.clues_seen_count * 100 %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
